<h1>Meus Projetos</h1>

<% if @projects.empty? %>
  <h2><%= t('.empty') %></h2>
  <h3><%= link_to(t('.first'), new_admin_project_path) %></h3>
<% else %>
  <% @projects.each do |project| %>
    <p><%= link_to project.title, admin_project_path(project) %></p>
  <% end %>
  <%= link_to t('.back'), root_path %>
<% end %>
